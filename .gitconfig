[alias]
	a = add
	aa = add -A
	aliases = "!git config -l | grep alias | cut -c 7-"
	ch = checkout
	chb = checkout -b
	cm = commit -m
	pick = cherry-pick
	amend = "commit --amend --no-edit"
	fpl = "!f() { git fetch origin $1:$1; }; f"
	lb="switch -"
	p = push
	pf = push --force
	po =  "!git push --set-upstream origin \"$(git rev-parse --abbrev-ref HEAD)\""
	pl = pull
	rb = rebase
	rbc = rebase --continue
	recent = !git switch $(git branch --sort=-committerdate | fzf | sed "s/^..//")
	rlc = "reset HEAD^"
	st = "status -s"
